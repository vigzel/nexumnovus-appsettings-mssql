<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup Label="Build">
    <TargetFramework>net6.0</TargetFramework>
  </PropertyGroup>

  <PropertyGroup Label="Package">
    <Product>NexumNovus.AppSettings.MsSql</Product>
    <Description>This package enables you to load and persist settings into MsSql database, and encrypt/decrypt settings marked with SecretSetting attribute.</Description>
    <PackageTags>nexumnovus;appsettings</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Data.SqlClient" Version="5.1.1" />
    <PackageReference Include="NexumNovus.AppSettings.Common" Version="0.3.0" />
  </ItemGroup>

</Project>
